@model WebSite.Models.Project.ProjectModel

@{
    Layout = null;
}

<div class="modal-dialog">
    <div class="modal-content">
        <div class="modal-header">
            <button type="button" class="close" data-dismiss="modal" aria-hidden="true">&times;</button>
            <h4 class="modal-title">@Resources.Resource.DeleteProjectQuestion</h4>
        </div>

        <div class="modal-body">
            <dl class="dl-horizontal">
                <dt> @Html.DisplayName(@Resources.Resource.Name) </dt>
                <dd> @Html.DisplayFor(model => model.Name) </dd>

                <dt> @Html.DisplayName(@Resources.Resource.Key) </dt>
                <dd> @Html.DisplayFor(model => model.Code) </dd>

                <dt> @Html.DisplayName(@Resources.Resource.Description) </dt>
                <dd> @Html.DisplayFor(model => model.Description) </dd>

                <dt> @Html.DisplayName(@Resources.Resource.Created) </dt>
                <dd> @Html.DisplayFor(model => model.DateCreate) </dd>

                <dt> @Html.DisplayName(@Resources.Resource.State) </dt>
                <dd> @Html.DisplayFor(model => model.NameStateProject) </dd>

                <dt> @Html.DisplayName(@Resources.Resource.Team) </dt>
                <dd> @Html.DisplayFor(model => model.NameTeamProject) </dd>
            </dl>

            @using (Html.BeginForm())
            {
                @Html.AntiForgeryToken()

                <div class="modal-footer">
                    <div class="col-md-offset-2 col-md-10">
                        <button type="submit" class="btn btn-primary">@Resources.Resource.Delete</button>
                        <button type="reset" class="btn btn-primary" data-dismiss="modal">@Resources.Resource.Close</button>
                    </div>
                </div>
             }
                </div>
            </div><!-- /.modal-content -->
</div><!-- /.modal-dialog -->
